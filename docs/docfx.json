{
  "metadata": [{
    "src": [{
      "files": ["**/bin/Release/ConsoleAppVisuals.dll"],
      "src": "../src/ConsoleAppVisuals/"
    }],
    "dest": "api",
    "properties": {
      "TargetFramework": "net7.0"
    }
  }],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
